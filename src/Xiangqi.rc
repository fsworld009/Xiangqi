        MENUITEM "載入棋譜(&O)...\tCtrl+O",         ID_FILE_OPEN
        MENUITEM "儲存棋譜(&S)\tCtrl+S",            ID_FILE_SAVE, GRAYED
    END
    POPUP "說明(&H)"
    BEGIN
        MENUITEM "關於 Xiangqi(&A)...",           ID_APP_ABOUT
    END
END


