IDR_MAINFRAME MENU 
BEGIN
    POPUP "遊戲(&G)"
    BEGIN
        MENUITEM "開新棋局(&N)\tCtrl+N",            ID_FILE_NEW
        MENUITEM "設定",                          ID_GAMESETTING
        MENUITEM SEPARATOR
        MENUITEM "結束(&X)",                      ID_APP_EXIT
    END
